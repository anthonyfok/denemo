msgid "C Major"
msgstr "C Dur"

#: src/commands.c:1650 src/commands.c:1652
#, no-c-format
msgid "F Major"
msgstr "F Dur"

#: src/commands.c:1661 src/commands.c:1663
#, no-c-format
msgid "MajorMinor"
msgstr "Dur/Moll"

#: src/commands.c:1661 src/commands.c:1663
#, no-c-format
msgid "Determine if the chord played is major or minor"
msgstr "Určit, zda je zahraný akord durový nebo molový"

