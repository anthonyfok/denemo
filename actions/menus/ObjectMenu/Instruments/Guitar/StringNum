<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <action>StringNum</action>
        <scheme>;; d-StringNum
(let ((num "0"))
(set! num (d-GetKeypress))
(d-DirectivePut-note-postfix "StringNum" (string-append "\\" num))
(d-DirectivePut-note-display "StringNum" num)
(d-DirectivePut-note-minpixels "StringNum" 20)
(d-DirectivePut-note-tx  "StringNum" 10)
(d-DirectivePut-note-gx  "StringNum" 5)
(d-DirectivePut-note-graphic"StringNum" "EncircleChar")
(d-Chordize #t)
(d-RefreshDisplay))</scheme>
        <label>String Number</label>
        <tooltip>Followed by a number, places string number on note.</tooltip>
      </row>
    </map>
  </merge>
</Denemo>
