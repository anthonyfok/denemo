<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <action>RehearsalMark</action>
        <scheme>;;; RehearsalMark
(d-DirectivePut-standalone-minpixels "RehearsalMark" 30)
(d-CursorLeft)
(d-DirectivePut-standalone-postfix "RehearsalMark"  " \\mark \\default" )
(d-DirectivePut-standalone-gx  "RehearsalMark"  0)
(d-DirectivePut-standalone-gy  "RehearsalMark"  -30)
(d-DirectivePut-standalone-graphic "RehearsalMark" "RehearsalMark")
(d-CursorRight)
(d-RefreshDisplay)</scheme>
        <label>Insert Rehearsal Mark</label>
        <tooltip>Inserts a rehearsal mark at the cursor.</tooltip>
      </row>
    </map>
  </merge>
</Denemo>
