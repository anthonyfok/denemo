<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>RTS</author>
    <map>
      <row type="scheme">
        <action>MovementSubtitle</action>
        <scheme>(SetHeaderField "subsubtitle")
(DenemoPrintAllHeaders)
(d-RefreshDisplay)</scheme>
        <_label>Subtitle</_label>
        <_tooltip>Prints the name given below the title of this movement.</_tooltip>
      </row>
    </map>
  </merge>
</Denemo>
