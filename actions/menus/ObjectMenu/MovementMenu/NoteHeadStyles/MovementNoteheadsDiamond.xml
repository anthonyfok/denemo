<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row type="scheme">
        <after>MovementNoteheadsHarmonic-mixed</after>
        <action>MovementNoteheadsDiamond</action>
        <scheme>(AttachDirective "layout" "postfix" "MovementNoteheads" "\t\\context {
\t\\Score		   
\t\\override NoteHead  #'style = #'diamond }\n")</scheme>
        <_label>Diamond</_label>
        <_tooltip>Diamond note head style for the current movement.</_tooltip>
      </row>
    </map>
  </merge>
</Denemo>
