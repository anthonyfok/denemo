<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row type="scheme">
        <after>MovementNoteheadsXcircle</after>
        <action>MovementNoteheadsXcircle</action>
        <scheme>(AttachDirective "layout" "postfix" "MovementNoteheads" "\t\\context {
\t\\Score		   
\t\\override NoteHead  #'style = #'triangle }\n")</scheme>
        <_label>Triangle</_label>
        <_tooltip>Triangle note head style for the current movement.</_tooltip>
      </row>
    </map>
  </merge>
</Denemo>
