<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <action>MovementPiece</action>
        <scheme>(SetHeaderField "piece")
(d-DirectivePut-paper-postfix "PrintAllHeaders" "printallheaders = ##t\n")
(d-RefreshDisplay)</scheme>
        <label>Piece</label>
        <tooltip>Prints the name given above and to the left at the start of the movement.</tooltip>
      </row>
    </map>
  </merge>
</Denemo>
