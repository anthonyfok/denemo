(d-CursorLeft)
(d-DirectivePut-standalone-postfix "SlurDirection" " \\slurUp" )
(d-DirectivePut-standalone-minpixels "SlurDirection" 40)
(d-CursorRight)
(d-RefreshDisplay) </scheme>
        <label>Slur Up</label>
        <tooltip>Prints slurs up</tooltip>
      </row>
    </map>
  </merge>
