<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <action>Third</action>
        <scheme>(define this-proc (lambda () 
         (if (d-NextNote) 
	      (begin (d-DiatonicShift "2") (this-proc)))))

  (d-DiatonicShift "2")
  (this-proc)
</scheme>
        <menupath>/ObjectMenu/InsertModeNote/Transpose/DiatonicShift</menupath>
        <menupath>/ObjectMenu/EditModeNote/Transpose/DiatonicShift</menupath>
        <menupath>/ObjectMenu/ClassicModeNote/Transpose/DiatonicShift</menupath>
        <menupath>/ObjectMenu/ModelessNote/Transpose/DiatonicShift</menupath>
        <label>Shift up a Third</label>
        <tooltip>Shifts all the notes from the current note up by two diatonic steps.</tooltip>
      </row>
    </map>
  </merge>
</Denemo>
