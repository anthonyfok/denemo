<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <action>ScoreArranger</action>
        <scheme>	(if (d-PreviousMovement)
		(d-WarningDialog "Returning to the first Movement to set the arranger"))
	(while (d-PreviousMovement)
		(display "Seeking first movement"))
        (SetHeaderField "arranger")
        (DenemoPrintAllHeaders)
        (d-RefreshDisplay)        
</scheme>
        <label>Arranger</label>
        <tooltip>Name of the arranger, flush-right below the composer.</tooltip>
      </row>
    </map>
  </merge>
</Denemo>
