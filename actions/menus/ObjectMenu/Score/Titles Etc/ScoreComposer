<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <action>ScoreComposer</action>
        <scheme>
	(if (d-PreviousMovement)
		(d-WarningDialog "Returning to the first Movement to set this composer"))
	(while (d-PreviousMovement)
		(display "Seeking first movement"))
        (SetHeaderField "composer")
        (d-DirectivePut-paper-postfix "PrintAllHeaders" "printallheaders = ##t\n")
        (d-RefreshDisplay)
        </scheme>
        <label>Composer</label>
        <tooltip>Prints name given to left above first movement of the score.</tooltip>
      </row>
    </map>
  </merge>
</Denemo>
