<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <action>ScorePoet</action>
        <scheme>	(if (d-PreviousMovement)
		(d-WarningDialog "Returning to the first Movement to set the Poet"))
	(while (d-PreviousMovement)
		(display "Seeking first movement"))
        (SetHeaderField "poet")
        (d-DirectivePut-paper-postfix "PrintAllHeaders" "printallheaders = ##t\n")
        (d-RefreshDisplay)        
</scheme>
        <label>Arranger</label>
        <tooltip>Sets the Poet/Lyricist </tooltip>
      </row>
    </map>
  </merge>
</Denemo>
