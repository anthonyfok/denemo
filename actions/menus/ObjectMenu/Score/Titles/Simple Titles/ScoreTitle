<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <after/>
        <action>ScoreTitle</action>
        <scheme>;;;ScoreTitle
	(if (d-PreviousMovement)
		(d-WarningDialog (_ "Returning to the first Movement to set this title")))
	(while (d-PreviousMovement)
		(display "Seeking first movement"))
        (SetHeaderField "title")
        (DenemoPrintAllHeaders)
        (d-RefreshDisplay)
        </scheme>
        <label>Title</label>
        <tooltip>The title of the music, centered just below the dedication.</tooltip>
      </row>
    </map>
  </merge>
</Denemo>
