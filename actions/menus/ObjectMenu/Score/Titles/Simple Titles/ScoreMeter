<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <after/>
        <action>ScoreMeter</action>
        <scheme>;;;ScoreMeter
	(if (d-PreviousMovement)
		(d-WarningDialog "Returning to the first Movement to set this."))
	(while (d-PreviousMovement)
		(display "Seeking first movement"))
        (SetHeaderField "meter")
        (DenemoPrintAllHeaders)
        (d-RefreshDisplay)
        </scheme>
        <label>Meter</label>
        <tooltip>Meter string, flush-left below the poet.</tooltip>
      </row>
    </map>
  </merge>
</Denemo>
