<?xml version="1.0" encoding="UTF-8"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <after>ClarinetInA</after>
        <action>CloseTransposingInstrument</action>
        <scheme>;;;CloseTransposingInstrument
(d-Directive-standalone "transposing")
(d-DirectivePut-standalone-postfix "transposing" "}")
(d-DirectivePut-standalone-display "transposing" "End Transposing")
(d-DirectivePut-standalone-override "transposing" DENEMO_OVERRIDE_TRANSPOSITION)
(d-DirectivePut-standalone-midibytes "transposing"  (d-StaffProperties "query=transposition"))
(d-DirectivePut-standalone-minpixels "transposing" 15)</scheme>
        <_label>Close Transposing Instrument</_label>
        <_tooltip>Returns MIDI output to transposition set on staff properties.</_tooltip>
      </row>
    </map>
  </merge>
</Denemo>
