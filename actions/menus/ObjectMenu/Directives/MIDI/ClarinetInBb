        <action>ClarinetInBb</action>
        <scheme>;;;;;;;;OpenClarinetInBflat
(d-Directive-standalone "transposing")
(d-DirectivePut-standalone-postfix "transposing" "\\markup \"Clarinet in B\\flat\"")
(d-DirectivePut-standalone-display "transposing" "Clarinet in Bb")
(d-DirectivePut-standalone-override "transposing" DENEMO_OVERRIDE_TRANSPOSITION)
(d-DirectivePut-standalone-midibytes "transposing" "-2")
(d-DirectivePut-standalone-minpixels "transposing" 30)
</scheme>
        <label>Clarinet in B flat</label>
        <tooltip>MIDI output is one tone lower than printed, inscribes clarinet in B flat above music</tooltip>
      </row>
