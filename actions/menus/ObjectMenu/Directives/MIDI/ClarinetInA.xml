<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <after>ClarinetInBb</after>
        <action>ClarinetInA</action>
        <scheme>;;;;;;;;OpenClarinetInA
(d-Directive-standalone "transposing")
(d-DirectivePut-standalone-postfix "transposing" "\\markup {Clarinet in A}")
(d-DirectivePut-standalone-display "transposing" "Clarinet in A")
(d-DirectivePut-standalone-override "transposing" DENEMO_OVERRIDE_TRANSPOSITION)
(d-DirectivePut-standalone-midibytes "transposing" "-3")
(d-DirectivePut-standalone-minpixels "transposing" 30)
(d-RefreshDisplay)</scheme>
        <_label>Clarinet in A</_label>
        <_tooltip>MIDI playback minor third lower than printed, marks as Clarinet in A</_tooltip>
      </row>
    </map>
  </merge>
</Denemo>
