  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <action>TypesettingOff</action>
        <scheme>
                     (StandAloneDirectiveProto (cons "TypesetOff" "
        	\\set Score.skipTypesetting = ##t       	
"))       
</scheme>
        <label>TypesettingOff</label>
        <tooltip>Turns off typesetting from that point until turned on.</tooltip>
      </row>
    </map>
  </merge>
</Denemo>
