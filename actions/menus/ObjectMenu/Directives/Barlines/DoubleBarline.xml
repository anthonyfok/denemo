<?xml version="1.0" encoding="UTF-8"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row type="scheme">
        <action>DoubleBarline</action>
        <scheme>;;;DoubleBarline
(StandAloneSelfEditDirective (cons "DoubleBarline" "\\bar \"||\"") #t "DoubleBarline")</scheme>
        <_label>Double Bar</_label>
        <_tooltip>Insert a double barline at cursor position.</_tooltip>
      </row>
    </map>
  </merge>
</Denemo>
