<?xml version="1.0" encoding="UTF-8"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <after>VoicePreset2</after>
        <action>VoicePreset3</action>
        <scheme>;;;VoicePreset3
(if (string=? (d-GetType) "Appending")
  (d-MoveCursorLeft))
(if (string=? (d-GetType) "STEMDIRECTIVE")
    (begin 
    	(d-MoveCursorRight)
    	(d-DeletePreviousObject)))
(d-StartUpStems)
(d-MoveCursorLeft)
(d-DirectivePut-stemdirective-graphic "VoiceSetting" "
v3
Serif
24
1
1")
(d-DirectivePut-stemdirective-postfix "VoiceSetting" "\\voiceThree")
(d-DirectivePut-stemdirective-override "VoiceSetting" DENEMO_OVERRIDE_LILYPOND)
(d-MoveCursorRight)
(d-RefreshDisplay)
</scheme>
        <_label>Voice Preset 3 (Stems Up)</_label>
        <_tooltip>Preset for third voice. Stems up.</_tooltip>
      </row>
    </map>
  </merge>
</Denemo>
