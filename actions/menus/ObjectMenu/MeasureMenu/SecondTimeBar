<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <after>FirstTimeBar</after>
        <action>SecondTimeBar</action>
        <scheme>;;;SecondTimeBar
(d-DirectivePut-standalone-minpixels "Repeat" 30)
(d-MoveCursorLeft)
(d-DirectivePut-standalone-postfix "Repeat" "
}{
	")

 (d-DirectivePut-standalone-graphic "Repeat" "SecondTimeBar")
 (d-DirectivePut-standalone-gy  "Repeat" -33)
(d-MoveCursorRight)
(d-RefreshDisplay)</scheme>
        <label>Second Time Bar</label>
        <tooltip>Closes first time bars and starts second time bar(s) (printing)</tooltip>
      </row>
    </map>
  </merge>
</Denemo>
