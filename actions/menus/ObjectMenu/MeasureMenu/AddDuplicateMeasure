<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <after>AddMeasure</after>
        <action>AddDuplicateMeasure</action>
        <scheme>(d-PushClipboard)
(d-SelectMeasure)
(d-Copy)
(if (d-MoveToMeasureRight)
	(if (None?)
		#t ; Empty measure
		(d-InsertMeasure)) ; create empty measure
	(begin ; End of staff
		(d-AppendMeasure)
		(d-MoveToMeasureRight)))
(d-Paste)
(d-PopClipboard)</scheme>
        <label>Insert Duplicate Measure After</label>
        <tooltip>Inserts a copy of the current measure after the present measure.</tooltip>
      </row>
    </map>
  </merge>
</Denemo>
