<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <after>SmallerStaff</after>
        <action>TinyStaff</action>
        <scheme>(ToggleDirective "staff" "prefix" "StaffSize" "\\new Staff \\with {\n
     		   fontSize = #-7\n
     		   \\override StaffSymbol #'staff-space = #(magstep -7)\n}\n &lt;&lt; {" DENEMO_OVERRIDE_LILYPOND)</scheme>
        <label>Tiny Staff</label>
        <tooltip>Prints current staff in a tiny font.</tooltip>
      </row>
    </map>
  </merge>
</Denemo>
