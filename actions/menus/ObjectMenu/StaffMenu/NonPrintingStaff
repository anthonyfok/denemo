<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <action>NonPrintingStaff</action>
        <scheme>;;;NonPrintingStaff
(if (d-DirectiveGetForTag-staff "NonPrintingStaff")
	(begin
 	(d-DirectiveDelete-staff "NonPrintingStaff"))
 	(begin 
		(d-DirectivePut-staff-prefix "NonPrintingStaff" " { } hiddenStaff = &lt;&lt; { ")
		(d-DirectivePut-staff-override "NonPrintingStaff" (logior DENEMO_OVERRIDE_LILYPOND DENEMO_OVERRIDE_GRAPHIC))
		(d-DirectivePut-staff-display "NonPrintingStaff" "Hidden Staff")))
(d-RefreshDisplay)


 </scheme>
        <label>Non-Printing Staff</label>
        <tooltip>Hides the staff in LilyPond print out.</tooltip>
      </row>
    </map>
  </merge>
</Denemo>
