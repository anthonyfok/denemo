<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <after>ChangeStaffUp</after>
        <action>ChangeStaffDown</action>
        <scheme>;;;ChangeStaffDown
(let ((staffname #f))
(if (d-DirectiveGet-standalone-display "ChangeStaff")
 	(d-DeleteObject))
 (if (equal? "STEMDIRECTIVE" (d-GetType))
 		(d-DeleteObject))
(d-PushPosition)
(d-MoveToStaffDown)
(set! staffname (d-StaffProperties "query=denemo_name"))
(d-PopPosition)
(StandAloneDirectiveProto (cons "ChangeStaff" (string-append "\\change Staff = \"" staffname "\" ")) #t #f "Staff Up")
(d-StartUpStems))
</scheme>
        <label>Change to Printing on Staff Below</label>
        <tooltip>Succeeding notes are printed on the staff below with stems up.</tooltip>
      </row>
    </map>
  </merge>
</Denemo>
