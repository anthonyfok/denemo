<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <action>NoTupletNumbers</action>
        <scheme>(d-DirectivePut-standalone-postfix "NoTupletNumbers" "\\override TupletNumber #'transparent = ##t")</scheme>
        <label>No Tuplet Numbers</label>
        <tooltip>Stop the printing of tuplet numbers from this point.</tooltip>
      </row>
    </map>
  </merge>
</Denemo>
