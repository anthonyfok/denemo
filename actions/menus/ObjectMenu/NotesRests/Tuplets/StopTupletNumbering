<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <action>StopTupletNumbering</action>
        <scheme>(d-DirectivePut-standalone-postfix "StopTupletNumbering" "\\override TupletNumber #'transparent = ##t ")</scheme>
        <label>Stop Tuplet Numbers</label>
        <tooltip>From this point the tuplet numbers will no longer be printed over tuplets.</tooltip>
      </row>
    </map>
  </merge>
</Denemo>
