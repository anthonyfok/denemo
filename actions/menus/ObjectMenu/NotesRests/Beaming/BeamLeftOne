    <map>
      <row>
        <after>BeamRightOne</after>
        <action>BeamLeftOne</action>
        <scheme>;;; BeamLeftOne
(BeamCount "Left" 1)</scheme>
        <label>One Beam Left</label>
        <tooltip>Prints with just one beam linking to left.</tooltip>
      </row>
    </map>
