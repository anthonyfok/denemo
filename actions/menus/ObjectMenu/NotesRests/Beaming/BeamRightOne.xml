<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <after>NoBeam</after>
        <action>BeamRightOne</action>
        <scheme>;;; BeamRightOne
(BeamCount "Right" 1)</scheme>
        <_label>One Beam Right</_label>
        <_tooltip>Prints with just one beam linking to the right.</_tooltip>
      </row>
    </map>
  </merge>
</Denemo>
