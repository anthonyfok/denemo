<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <action>ShiftUp</action>
        <scheme>(SingleAndSelectionSwitcher (lambda () 
	(ShiftProto ANS::CalculateDiatonicStepUp)))
</scheme>
        <label>Tonal step up</label>
        <tooltip>Transpose/shift the cursor note or selection one step up, stay in the keysignature.</tooltip>
      </row>
    </map>
  </merge>
</Denemo>
