<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <action>OpenOctaveUp</action>
        <scheme> ;;;;OpenOctaveUp
(d-DirectivePut-standalone-postfix "OpenPrintTransposed" "\\transpose c c' {")
(d-CursorLeft)
(d-DirectivePut-standalone-display "OpenPrintTransposed" "Up8va")
(d-RefreshDisplay) </scheme>
        <label>(Print) Open { Octave Up</label>
        <tooltip>Opens a block to be transposed up one octave
	This must be followed by a CloseBlock "}" to end the transposed section.</tooltip>
      </row>
    </map>
  </merge>
</Denemo>
