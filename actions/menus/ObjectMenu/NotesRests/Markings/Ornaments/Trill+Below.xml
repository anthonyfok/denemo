<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <action>Trill+Below</action>
        <scheme>(d-InsertLilyDirective "directive=_\"+\"\0display=Tr+Below")
</scheme>
        <_label>18th c Trill Below</_label>
        <_tooltip>Inserts 18th c style trill below note</_tooltip>
      </row>
    </map>
  </merge>
</Denemo>
