<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <after>ToggleReverseTurn</after>
        <action>ToggleSegno</action>
        <scheme>;;;ToggleSegno
        (ChordAnnotation "ToggleSegno" "\\segno"    ToggleSegno::params    LG-Segno)
</scheme>
        <_label>Segno (Off/On)</_label>
        <_tooltip>Adds/Removes a Segno to the note</_tooltip>
      </row>
    </map>
  </merge>
</Denemo>
