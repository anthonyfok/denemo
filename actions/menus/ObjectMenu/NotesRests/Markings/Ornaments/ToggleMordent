    <map>
      <row>
        <action>ToggleMordent</action>
        <scheme>;;;ToggleMordent
				(ChordAnnotation "ToggleMordent" "\\mordent"    ToggleMordent::params    LG-Mordent)
</scheme>
        <label>Mordent (Off/On)</label>
        <tooltip>Adds/Removes a Mordent to the note</tooltip>
      </row>
    </map>
  </merge>
