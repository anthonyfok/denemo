<?xml version="1.0" encoding="UTF-8"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <action>Finger3</action>
        <scheme>(ToggleDirective "note" "postfix" "Fingering" "-3 ")
(if (d-Directive-note? "Fingering")
	(d-DirectivePut-note-display"Fingering" "3"))
(d-Chordize #t)
</scheme>
        <_label>Finger 3</_label>
        <_tooltip>Inserts fingering for finger 3</_tooltip>
      </row>
    </map>
  </merge>
</Denemo>
