  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <action>Finger1</action>
        <scheme>(ToggleDirective "note" "postfix" "Fingering" "-1 ")
(if (d-Directive-note? "Fingering")
	(d-DirectivePut-note-display"Fingering" "1"))
(d-Chordize #t)
</scheme>
        <label>Finger 1</label>
        <tooltip>Inserts fingering for finger 1</tooltip>
      </row>
    </map>
  </merge>
