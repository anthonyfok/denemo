<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <action>Finger4</action>
        <scheme>(ToggleDirective "note" "postfix" "Fingering" "-4 ")
(if (d-Directive-note? "Fingering")
	(d-DirectivePut-note-display"Fingering" "4"))
(d-Chordize #t)
</scheme>
        <_label>Finger 4</_label>
        <_tooltip>Inserts fingering for finger 4</_tooltip>
      </row>
    </map>
  </merge>
</Denemo>
