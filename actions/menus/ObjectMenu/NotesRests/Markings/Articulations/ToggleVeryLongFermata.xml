<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <action>ToggleVeryLongFermata</action>
        <scheme>;;;VeryLongFermata
        (ChordAnnotation "ToggleVeryLongFermata" "\\verylongfermata"    ToggleVeryLongFermata::params    LG-VeryLongFermata)
</scheme>
        <_label>VeryLongFermata (Off/On)</_label>
        <_tooltip>Mark/Unmark the current chord with very long fermata</_tooltip>
      </row>
    </map>
  </merge>
</Denemo>
