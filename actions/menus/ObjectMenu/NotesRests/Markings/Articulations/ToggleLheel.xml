<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row type="scheme">
        <scheme>;;;ToggleLheel
        (ChordAnnotation "ToggleLheel" "\\lheel"    ToggleLheel::params    LG-Lheel)
</scheme>
