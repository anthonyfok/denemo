<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row type="scheme">
        <after/>
        <action>Fortissimo</action>
        <scheme>   ;;; ff
(Dynamics::Put "Fortissimo" "\\ff" "120")
</scheme>
        <_label>Fortissimo</_label>
        <_tooltip>Places a ff sign on the chord at the cursor</_tooltip>
      </row>
    </map>
  </merge>
</Denemo>
