<?xml version="1.0" encoding="UTF-8"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row type="scheme">
        <after>MezzoForte</after>
        <action>Largo</action>
        <scheme>;;;Tempo
(d-DirectivePut-chord-postfix "Tempo" "^\\markup \\italic \\bold {Largo}")
(d-DirectivePut-chord-display "Tempo" "Largo")
(d-DirectivePut-chord-midibytes "Tempo" "")
(d-RefreshDisplay)
;;;End of scheme script</scheme>
        <_label>Largo</_label>
        <_tooltip>Print Largo above note at cursor</_tooltip>
      </row>
    </map>
  </merge>
</Denemo>
