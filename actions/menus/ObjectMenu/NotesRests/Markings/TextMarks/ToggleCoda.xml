<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <action>ToggleCoda</action>
        <scheme>;;;ToggleCoda
        (ChordAnnotation "ToggleCoda" "\\coda"    ToggleCoda::params    LG-Coda)
</scheme>
        <_label>Coda (Off/On)</_label>
        <_tooltip>Adds/Removes a Coda to the note</_tooltip>
      </row>
    </map>
  </merge>
</Denemo>
