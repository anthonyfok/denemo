<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <action>Glissando</action>
        <scheme>;;;Glissando
(d-DirectivePut-chord-display "Glissando"  "Gliss" )
(d-DirectivePut-chord-postfix "Glissando"  "\\glissando")
