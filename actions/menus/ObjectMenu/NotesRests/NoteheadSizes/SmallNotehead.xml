<?xml version="1.0" encoding="UTF-8"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <action>SmallNotehead</action>
        <scheme>;;;SmallNotehead
(let ((tag "SmallNotehead"))
	(if (d-Directive-note? tag)
		(d-DirectiveDelete-note tag)
		(begin
		(d-DirectivePut-note-display tag "Small")
		
		(d-DirectivePut-note-prefix tag "\\small ")))
	(d-SetSaved #f))

</scheme>
        <_label>(Print) Small  Size</_label>
        <_tooltip>Prints out note heads in small size</_tooltip>
      </row>
    </map>
  </merge>
</Denemo>
