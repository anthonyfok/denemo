<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <action>InsertOneNote</action>
        <scheme>;;;InsertOneNote
(if (and (Appending?) (d-GetNonprinting))
	(eval-string (string-append "(d-" (string-upcase (d-GetCursorNote)) ")"))
	(eval-string (string-append "(d-Insert" (number-&gt;string (d-GetPrevailingDuration)) ")")))</scheme>
        <label>Insert Note</label>
        <tooltip>Inserts a note in the prevailing duration.</tooltip>
      </row>
    </map>
  </merge>
</Denemo>
