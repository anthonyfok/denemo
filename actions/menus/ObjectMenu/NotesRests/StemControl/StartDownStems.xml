<?xml version="1.0" encoding="UTF-8"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <action>StartDownStems</action>
        <scheme>
		(d-InsertStem)
		(d-MoveCursorLeft)
		(d-StemDown)
		(d-MoveCursorRight)
	</scheme>
        <_label>Stems Down</_label>
        <_tooltip>Stems from now on point downward.</_tooltip>
      </row>
    </map>
  </merge>
</Denemo>
