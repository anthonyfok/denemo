<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row type="scheme">
        <action>StartUpStems</action>
        <scheme>
		(d-InsertStem)
		(d-MoveCursorLeft)
		(d-StemUp)
		(d-MoveCursorRight)
	</scheme>
        <_label>Stems Up</_label>
        <_tooltip>Stems from now on point upward.</_tooltip>
      </row>
    </map>
  </merge>
</Denemo>
