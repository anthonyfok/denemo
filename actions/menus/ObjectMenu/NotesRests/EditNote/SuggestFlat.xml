<?xml version="1.0" encoding="UTF-8"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row type="scheme">
        <after>SuggestSharp</after>
        <action>SuggestFlat</action>
        <scheme>(d-DirectivePut-chord-postfix "SuggestedAccidental" "^\\markup \\flat")
(d-DirectivePut-chord-gy "SuggestedAccidental" -100)
(d-DirectivePut-chord-graphic "SuggestedAccidental" "
\xE2\x99\xAD
Sans
30")</scheme>
        <_label>Suggest Flat</_label>
        <_tooltip>Prints flat sign over note</_tooltip>
      </row>
    </map>
  </merge>
</Denemo>
