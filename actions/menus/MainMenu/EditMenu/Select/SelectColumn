<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <action>SelectColumn</action>
        <scheme>(d-PushPosition)
(d-UnsetMark)
(let loop ()
	(if (d-MoveToStaffUp)
		(loop)
		#t))
(let loop2 ()
	(if (d-StaffDown)
		(loop2)
		#t))			
(d-PopPosition)</scheme>
        <label>Select Column</label>
        <tooltip>Selects the whole column</tooltip>
      </row>
    </map>
  </merge>
</Denemo>
