<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <after>PasteClipboard</after>
        <action>FindNextLowerNote</action>
        <scheme> ;;;;;;;; FindNextNoteLower
(let loop ((lowest (d-GetNoteAsMidi)) (current 0))
  (set! current (d-GetNoteAsMidi))
  (if (or (zero? current) (&gt;= current lowest))
      (begin
	(if (d-CursorRight)
	    (loop lowest current)
	    #t))))
      

  </scheme>
        <label>Next Lower</label>
        <tooltip>Moves cursor to next note lower than the current note.</tooltip>
      </row>
    </map>
  </merge>
</Denemo>
