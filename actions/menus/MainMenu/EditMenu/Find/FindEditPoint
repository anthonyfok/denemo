<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <after>NextHigher</after>
        <action>FindEditPoint</action>
        <scheme>  ;;;;;; FindEditPoint
(let loop ( )
  (define current (d-PrevNote))
  (if (and current (d-GetNonprinting))
      (loop)))
(d-NextNote)
(d-RefreshDisplay)
;;;;;;;;;;;;;;;;;

</scheme>
        <label>Find Edit Point</label>
        <tooltip>Searches backward for the start of non-printing notes.</tooltip>
      </row>
    </map>
  </merge>
</Denemo>
