<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <action>DrumGm2User</action>
        <scheme>(if (defined? 'DrumHash)
  (d-PutNoteName (hashq-ref DrumHash (string-&gt;symbol (d-GetLowestNote))))
  (begin 
    (display "\nNo DrumHash existent. Force Reload!\n")
    (d-ReloadDrumHash)
    (d-PutNoteName (hashq-ref DrumHash (string-&gt;symbol (d-GetLowestNote))))
  )
)
(d-RefreshDisplay)</scheme>
        <label>Convert GM Drums to User Notation</label>
        <tooltip>Converts a note from GM Drum Notation to users Drum Notation, defined in an external .denemo file (see manual)</tooltip>
      </row>
    </map>
  </merge>
</Denemo>
