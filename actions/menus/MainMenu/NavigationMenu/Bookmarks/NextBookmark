  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <action>NextBookmarkR</action>
        <scheme>;;;NextBookMarkR by Nils Gey
(if (d-DirectiveGet-standalone-display "RehearsalMark")
(d-MoveCursorRight) 
)
(let gotoBookmark ()
 (if  (and (not (d-DirectiveGet-standalone-display "RehearsalMark")) (d-MoveCursorRight) )
	(gotoBookmark))
)</scheme>
        <label>Next Bookmark</label>
        <tooltip>Seach forward for bookmarks in the current staff.</tooltip>
      </row>
    </map>
  </merge>
