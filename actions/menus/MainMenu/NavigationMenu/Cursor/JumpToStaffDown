<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <after>JumpToStaffUp</after>
        <action>JumpToStaffDown</action>
        <scheme>(if (d-GoToPosition #f (1+ (d-GetStaff)) #f (floor (duration::GetMetricalPosition)))
	#t
	(d-MoveToStaffDown))</scheme>
        <label>Move to Staff Down</label>
        <tooltip>Move the cursor one staff down. Try to remain on the metrical position.</tooltip>
      </row>
    </map>
  </merge>
</Denemo>
