<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row type="scheme">
        <after>CursorUp</after>
        <action>JumpLeft</action>
        <scheme>(if (d-PrevObjectInMeasure) 
	(if (d-MoveToMeasureLeft) (d-MoveToMeasureRight))
	(d-MoveToMeasureLeft))</scheme>
        <_label>JumpLeft</_label>
        <_tooltip>Jumps to beginning of this measure or previous measure.</_tooltip>
      </row>
    </map>
  </merge>
</Denemo>
