<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <action>GoToMeasureEnd</action>
        <scheme>
(let loop ()
    (if  (or (None?) (Appending?))
	#t
	(begin (d-MoveCursorRight) (loop))))
		</scheme>
        <_label>Go To End of Measure</_label>
        <_tooltip/>
      </row>
    </map>
  </merge>
</Denemo>
