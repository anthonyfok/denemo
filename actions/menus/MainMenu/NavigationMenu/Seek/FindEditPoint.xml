<?xml version="1.0" encoding="UTF-8"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row type="scheme">
        <after>NextHigher</after>
        <action>FindEditPoint</action>
        <scheme>  ;;;;;; FindEditPoint
(let loop ( )
  (define current (d-PrevNote))
  (if (and current (d-GetNonprinting))
      (loop)))
 (if (not (d-GetNonprinting))
(d-NextNote))
(d-RefreshDisplay)
;;;;;;;;;;;;;;;;;

</scheme>
        <_label>Find Edit Point</_label>
        <_tooltip>Searches backward for the start of non-printing notes.</_tooltip>
      </row>
    </map>
  </merge>
</Denemo>
