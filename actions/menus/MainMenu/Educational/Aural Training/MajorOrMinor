<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <action>MajorOrMinor</action>
        <scheme>
(define ChordComparison::Major (cons "Major" "c e g"))
(define ChordComparison::Minor (cons "Minor" "c ees g"))

(ChordComparison::ChordComparison (list ChordComparison::Major ChordComparison::Minor))

</scheme>
        <label>MajorMinor</label>
        <tooltip>Determine if the chord played is major or minor</tooltip>
      </row>
    </map>
  </merge>
</Denemo>
