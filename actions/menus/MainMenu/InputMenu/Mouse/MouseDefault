        <action>MouseDefault</action>
        <scheme>;;MouseDefault
 (d-AddKeybinding "NoOp" "PrsL-CapsLock")
(d-AddKeybinding "A" "a")
(d-AddKeybinding "B" "b")
(d-AddKeybinding "C" "c")
(d-AddKeybinding "D" "d")
(d-AddKeybinding "E" "e")
(d-AddKeybinding "F" "f")
(d-AddKeybinding "G" "g")
(d-AddKeybinding "Shft+0" "Change0")
(d-AddKeybinding "Shft+1" "Change1")
(d-AddKeybinding "Shft+2" "Change2")
(d-AddKeybinding "Shft+3" "Change3")
(d-AddKeybinding "Shft+4" "Change4")
(d-AddKeybinding "Shft+5" "Change5")
(d-AddKeybinding "Shft+6" "Change6")
(d-DirectiveDelete-score "Button0")
(d-DirectiveDelete-score "Button1")
(d-DirectiveDelete-score "Button2")
(d-DirectiveDelete-score "Button3")
(d-DirectiveDelete-score "Button4")
(d-DirectiveDelete-score "Button5")
(d-DirectiveDelete-score "Button6")
(d-DirectiveDelete-score "Dot")
(d-DirectiveDelete-score "XDot")

(d-DirectiveDelete-score "Rest0")
(d-DirectiveDelete-score "Rest1")(d-DirectiveDelete-score "Rest2")(d-DirectiveDelete-score "Rest3")
(d-DirectiveDelete-score "Rest4")(d-DirectiveDelete-score "Rest5")(d-DirectiveDelete-score "Rest6")

(d-DirectiveDelete-score "BeginSlur")
(d-DirectiveDelete-score "EndSlur")
(d-DirectiveDelete-score "Tie")
(d-DirectiveDelete-score "Triplets")</scheme>
        <label>Mouse Default Action</label>
        <tooltip>Mouse left click positions the cursor, right click edits, left-click with caps-lock does nothing..</tooltip>
      </row>
    </map>
