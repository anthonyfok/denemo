<?xml version="1.0"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row>
        <action>MouseInsertion</action>
        <scheme>;;MouseInsertion
        (d-AddKeybinding "InsertOneNote" "PrsL-CapsLock")
(d-AddKeybinding "A" "a")
(d-AddKeybinding "B" "b")
(d-AddKeybinding "C" "c")
(d-AddKeybinding "D" "d")
(d-AddKeybinding "E" "e")
(d-AddKeybinding "F" "f")
(d-AddKeybinding "G" "g")
(d-AddKeybinding "Set0" "Shft+0")
(d-AddKeybinding "Set1" "Shft+1")
(d-AddKeybinding "Set2" "Shft+2")
(d-AddKeybinding "Set3" "Shft+3")
(d-AddKeybinding "Set4" "Shft+4")
(d-AddKeybinding "Set5" "Shft+5")
(d-AddKeybinding "Set6" "Shft+6")

(CreateButton "Button0" "&#x1D15D;")
(d-SetDirectiveTagActionScript "Button0" "(d-Set0)")
(CreateButton "Button1" "&#x1D15E;")
(d-SetDirectiveTagActionScript "Button1" "(d-Set1)")
(CreateButton "Button2" "&#x1D15F;")
(d-SetDirectiveTagActionScript "Button2" "(d-Set2)")
(CreateButton "Button3" "&#x1D160;")
(d-SetDirectiveTagActionScript "Button3" "(d-Set3)")
(CreateButton "Button4" "&#x1D161;")
(d-SetDirectiveTagActionScript "Button4" "(d-Set4)")
(CreateButton "Button5" "&#x1D162;")
(d-SetDirectiveTagActionScript "Button5" "(d-Set5)")
(CreateButton "Button6" "&#x1D163;")
(d-SetDirectiveTagActionScript "Button6" "(d-Set6)")

(CreateButton "Dot" "&#x2022;")
(d-SetDirectiveTagActionScript "Dot" "(d-AddDot)")

(CreateButton "XDot" "&#x2022; X")
(d-SetDirectiveTagActionScript "XDot" "(d-RemoveDot)")

(CreateButton "Rest0" "&#x1D13B;")
(d-SetDirectiveTagActionScript "Rest0" "(d-InsertWholeRest)")
(CreateButton "Rest1" "&#x1D13C;")
(d-SetDirectiveTagActionScript "Rest1" "(d-InsertHalfRest)")
(CreateButton "Rest2" "&#x1D13D;")
(d-SetDirectiveTagActionScript "Rest2" "(d-InsertQuarterRest)")

(CreateButton "Rest3" "&#x1D13E;")
(d-SetDirectiveTagActionScript "Rest3" "(d-InsertEighthRest)")
(CreateButton "Rest4" "&#x1D13F;")
(d-SetDirectiveTagActionScript "Rest4" "(d-InsertSixteenthRest)")
(CreateButton "Rest5" "&#x1D140;")
(d-SetDirectiveTagActionScript "Rest5" "(d-InsertThirtysecondRest)")
(CreateButton "Rest6" "&#x1D141;")
(d-SetDirectiveTagActionScript "Rest6" "(d-InsertSixtyfourthRest)")

(CreateButton "BeginSlur" "(")
(d-SetDirectiveTagActionScript "BeginSlur" "(d-ToggleBeginSlur)")
(CreateButton "EndSlur" ")")
(d-SetDirectiveTagActionScript "EndSlur" "(d-ToggleEndSlur)")

(CreateButton "Tie" "&#x260A;")
(d-SetDirectiveTagActionScript "Tie" "(d-ToggleTie)")

(CreateButton "Triplets" "3")
(d-SetDirectiveTagActionScript "Triplets" "(d-ToggleTripleting)")
</scheme>
        <label>Mouse Note Insertion</label>
        <tooltip>Changes the mouse use when CapsLock is on: left click inserts note in the prevailing duration. The shortcuts for number keys 0, 1, 2 ... are set to change the prevailing duration.</tooltip>
      </row>
    </map>
  </merge>
</Denemo>
