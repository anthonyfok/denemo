<?xml version="1.0" encoding="UTF-8"?>
<Denemo>
  <merge>
    <title>A Denemo Keymap</title>
    <author>AT, JRR, RTS</author>
    <map>
      <row type="scheme">
        <action>ShowVirtualKeyboard</action>
        <after>SimulateMidi</after>
        <_label>Virtual MIDI Keyboard</_label>
        <_tooltip>Displays a piano style keyboard that can be clicked on to enter notes like a real MIDI device. Right click adds notes to the chord.</_tooltip>
      </row>
    </map>
  </merge>
</Denemo>
